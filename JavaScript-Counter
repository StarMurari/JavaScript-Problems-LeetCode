/**
 * @param {number} n
 * @return {Function} counter
 */
var createCounter = function(n) {
    
    let count = n;
    return function() {
        for(let i=0;i<3;i++)
            return n++;
    };
};

/** 
 * const counter = createCounter(10)
 * counter() // 10
 * counter() // 11
 * counter() // 12
 */
